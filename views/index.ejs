<body>
	<header class="logo-matise">
		<img src="public/img/logoWhite.svg" alt="Logo van Matise">
		<div class="menu-icon icon">
			<hr align="right">
			<hr align="right">
		</div>
	</header>
	<section class="home">
		<%- include('home') %>
	</section>
	<section class="toc" id="inhoud">
